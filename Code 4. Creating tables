CREATE TABLE geo.climate_eez_waterarea (
	climate_zones varchar(100) NULL,
	eez_id int4 NULL,
	area float8 NULL,
	CONSTRAINT climate_eez_waterarea_pk PRIMARY KEY (id)
);
WITH PRIMARY KEY 


CREATE TABLE geo.climate_eez_waterarea (
	climate_zones varchar(100) NULL,
	eez_id int4 NULL,
	area float8 NULL
);

WITHOUT PRIMARY KEY


CREATE TABLE geo.climatezone_newcell_x_fao (
	climate_zones varchar(100) NULL,
